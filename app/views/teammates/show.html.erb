<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', teammates_path  %></li>
      <li><%= link_to 'New', new_teammate_path %></li>
      <li class="active"><%= link_to 'Show', teammate_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>User_id:</b>
          <%= @teammate.user_id %>
        </p>

        <p>
          <b>Group_id:</b>
          <%= @teammate.group_id %>
        </p>

        <p>
          <b>Manager_id:</b>
          <%= @teammate.manager_id %>
        </p>

        <p>
          <b>Status:</b>
          <%= @teammate.status %>
        </p>

        <p>
          <b>Accepted_at:</b>
          <%= @teammate.accepted_at %>
        </p>

        <p>
          <b>Teammate_code:</b>
          <%= @teammate.teammate_code %>
        </p>

        <p>
          <b>Deleted_at:</b>
          <%= @teammate.deleted_at %>
        </p>
      <%= link_to label_name(:edit), edit_teammate_path(@teammate) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>