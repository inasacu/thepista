<%	
	request_image, first_icon, request_link, the_label, the_icon, item_group_link, the_match = upcoming_matches(matches)
	the_time = ago(the_match.created_at)
%>

<%= render 'home/upcoming_template', 
			:request_image => request_image, 
			:first_icon => first_icon, 
			:request_link => request_link, 
			:the_label => the_label, 
			:the_icon => the_icon, 
			:item_group_link => item_group_link, 
			:the_time => the_time %>




