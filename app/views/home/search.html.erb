<%
if current_user
	the_label = label_name(:search_result)
	content_for(:title, the_label)
	
	block_model = ['cup', 'challenge']
%> 
	<div class="block">
		
		<%= get_secondary_navigation %>
		
		<div class="content">   
			<h2 class="title"><%= the_label %></h2>
			
			<div class="inner">
				<%= get_class_table_id_controller %>
			
				<% @item_results.each do |item| 
					the_class = item.class.to_s.downcase
				%>
					<%= render "search_#{item.class.to_s.downcase}", :item => item unless block_model.include?(the_class)%>
				<% end -%>
				
				</table>
	    	</div>
			
			</div>
		</div>
	</div>
	
	<% content_for :sidebar, render(:partial => 'home/sidebar_home') -%>
<% 
else 
%>
	<%= render 'home/details' %>	
<% 
end 
-%>

