<head>

	<meta charset="utf-8" />
	
	<!-- Set the viewport width to device width for mobile -->

	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<meta http-equiv="Content-Language" content="<%= I18n.locale %>"/>
	<meta name="Author" content="HayPista"/> 	
	<meta name="title" content="<%= page_title %>.  <%= I18n.t(:modo_key_title) %>" />
	<meta name="description" content="<%= I18n.t(:modo) %>">		
	<meta name="keywords" content="<%= I18n.t(:modo_keywords) %>"/> 

	<meta name="vw96.objectype" content="Document" />
	<meta name="resource-type" content="Document" />
	<meta name="distribution" content="all" />
	<meta name="robots" content="all" />
	<meta name="revisit" content="15 days" />
	<meta name="google-site-verification" content="qyTyrJH8o08Ur15mr5hUYjVNyWcbfQatF3yar3Ry2ws" />
	<meta name="google-site-verification" content="OJzCfMpdsascUFtDoxXJpZGEIsjXjKG1691P1FC5pIc" />

	<title><%= page_title %></title>

	<link rel="image_src" href="http://haypista.com/images/haypista.png" />
	<link  rel="apple-touch-icon" href="/apple-touch-icon.png" />
	<link rel="home" href="http://www.haypista.com">
	<link rel="shortcut icon" type="image/ico" href="/favicon.ico">
  

	<% if DISPLAY_DEFAULTS %>	
	<%= javascript_include_tag :defaults %>		
	<% end -%>

	<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js" %>	
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?v=3.3&sensor=true&Language=es"></script> 

	<%= ajaxful_rating_style %>
	<%= ajaxful_rating_script %>

	<%= javascript_include_tag 'jquery.google-analytics', 'jquery.notifyBar', 'jquery.cluetip', 'jquery.hoverIntent' %>
	<%= javascript_include_tag 'jquery.clearinput' %>
	<%= javascript_include_tag "jquery.cook" %>	
	<%= javascript_include_tag "gmap3" %>	

	<%= javascript_include_tag 'application' %>	
	<%= javascript_include_tag "vtip" %>

	<%= stylesheet_link_tag 'vtip' %>
	
	<%= render :partial => 'utilities/google_analytics' %>
	
	<!-- Included CSS Files -->

	<%= stylesheet_link_tag 'zurb/globals' %>
	<%= stylesheet_link_tag 'zurb/typography' %>
	<%= stylesheet_link_tag 'zurb/grid' %>
	<%= stylesheet_link_tag 'zurb/ui' %>
	<%= stylesheet_link_tag 'zurb/forms' %>
	<%= stylesheet_link_tag 'zurb/orbit' %>
	<%= stylesheet_link_tag 'zurb/reveal' %>
	<%= stylesheet_link_tag 'zurb/app' %>
	<%= stylesheet_link_tag 'zurb/mobile' %>
	<%= stylesheet_link_tag 'zurb/presentation' %>
	<%= stylesheet_link_tag 'zurb/app' %>
	<%#= stylesheet_link_tag 'zurb/haypista' %>
	
	<%#= stylesheet_link_tag 'zurb/notably' %>
	
	<%#= stylesheet_link_tag 'zurb/verify' %>
	<%#= stylesheet_link_tag 'zurb/verify_app' %>
	<%#= stylesheet_link_tag 'zurb/marketing' %>
	<%#= stylesheet_link_tag 'zurb/verify_annotatable' %>
	
	
	<%#= stylesheet_link_tag "http://www.zurb.com/blog/assets/all.css" %>
	<%#= stylesheet_link_tag "http://www.zurb.com/blog/assets/public.css" %>

	<!--[if lt IE 9]>
		<link rel="stylesheet" href="../../stylesheets/zurb/ie.css">
	<![endif]-->


	<!-- IE Fix for HTML5 Tags -->
	<!--[if lt IE 9]>
		<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->


	<%= javascript_include_tag "vtip" %>
	<%= javascript_include_tag "zurb/jquery-1.5.1.min" %>
	<%= javascript_include_tag "zurb/jquery.reveal" %>
	<%= javascript_include_tag "zurb/jquery.orbit-1.3.0" %>
	<%= javascript_include_tag "zurb/forms.jquery" %>
	<%= javascript_include_tag "zurb/jquery.customforms" %>
	<%= javascript_include_tag "zurb/jquery.placeholder.min" %>
	<%= javascript_include_tag "zurb/app" %>
	<%= javascript_include_tag "zurb/presentation" %>
	  

</head>

<body id="-page" class="">

	<%= render 'shared/header_zurb' %>

	<div class="container">
		
		<%= render 'shared/flash_zurb' %>

		<%= render 'shared/wrapper_zurb' %>

		<%#= render 'templates/twelve_columns_zurb' %>
		<%#= render 'templates/eight_columns_zurb' %>

		<%#= render 'shared/tabs_mobile_show_zurb' %>
		<%= render 'shared/footer_zurb' %>

	</div>

</body>

</html>