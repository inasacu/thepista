<div class="block">

  <%= render :partial => 'secondary_navigation' %>

         
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Blog_id:</b>
          <%= @entry.blog_id %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @entry.user_id %>
        </p>

        <p>
          <b>Group_id:</b>
          <%= @entry.group_id %>
        </p>

        <p>
          <b>Title:</b>
          <%= @entry.title %>
        </p>

        <p>
          <b>Body:</b>
          <%= @entry.body %>
        </p>

        <p>
          <b>Comments_count:</b>
          <%= @entry.comments_count %>
        </p>

      <%= link_to label_name(:edit), edit_entry_path(@entry) %>

    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>