<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', classifieds_path  %></li>
      <li><%= link_to 'New', new_classified_path %></li>
      <li class="active"><%= link_to 'Show', classified_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Message:</b>
          <%= @classified.message %>
        </p>

        <p>
          <b>Item_type:</b>
          <%= @classified.item_type %>
        </p>

        <p>
          <b>Item_id:</b>
          <%= @classified.item_id %>
        </p>

        <p>
          <b>Starts_at:</b>
          <%= @classified.starts_at %>
        </p>

        <p>
          <b>Ends_at:</b>
          <%= @classified.ends_at %>
        </p>
      <%= link_to 'Edit', edit_classified_path(@classified) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>