<div class="block">

  <%= render :partial => '/groups/secondary_navigation' %>   
  
  <div class="content">
	<p>
		<b><%= link_to "", classified_path(:id => @next), :class => 'previous_classified' if @next %></b>
	</p>
	<p>
		<b><%= link_to "", classified_path(:id => @previous), :class => 'next_classified' if @previous %></b>
	</p>
	<br />
	
	<h2 class="title"><%= h @classified.concept %></h2>
	
	<div class="inner">
		<table class="table">			
			<%= render :partial => 'classified', :locals => {:full_classified => true} %>
		</table>
	</div>
  </div>
</div>

<%# content_for :sidebar, render(:partial => 'sidebar_show') if current_user.is_manager_of?(Group.find(@classified.table_id)) -%>
<% content_for :sidebar, render(:partial => '/groups/sidebar_show') -%>