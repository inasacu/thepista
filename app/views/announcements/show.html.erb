<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', announcements_path  %></li>
      <li><%= link_to 'New', new_announcement_path %></li>
      <li class="active"><%= link_to 'Show', announcement_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Message:</b>
          <%= @announcement.message %>
        </p>

        <p>
          <b>Start_at:</b>
          <%= @announcement.starts_at %>
        </p>

        <p>
          <b>Ends_at:</b>
          <%= @announcement.ends_at %>
        </p>
      <%= link_to 'Edit', edit_announcement_path(@announcement) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>