<% 		
	get_first_active ||=""
	get_first_active = get_active("#{get_the_controller}_#{get_the_action}") if is_controller('enchufado') and is_action('index') 
	
	the_label = "" 
	the_action = ['show','edit', 'team list', 'schedule list', 'enchufado current list', 'enchufado previous list']
	the_controller = ['fee', 'payment', 'schedule']
	
	is_single_enchufado = false
	if @enchufado 
		is_single_enchufado = true
	end
	content_for(:description, "#{@enchufado.name}") if @enchufado
%>
	<%= set_tab_navigation(link_to(label_name(:enchufados), enchufados_path, set_tab_navigation_active('enchufado_index'))) unless @enchufado %>
	<%= set_tab_navigation(item_name_link(@enchufado, set_tab_navigation_active('enchufado_show'))) if is_single_enchufado %>
<% 	
	if the_action.include?(get_the_action) or the_controller.include?(get_the_controller)
		if is_current_member_of(@enchufado) 
%>		
		<%= content_tag :dd, link_to(label_name(:users) , team_list_path(:id => @enchufado), :class => get_active('enchufado_team_list')) %>

<%			
		end
	end
%>
