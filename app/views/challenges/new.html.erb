<div class="block">

	<%= render :partial => 'secondary_navigation' %>

	<div class="content">  

		<h2 class="title"><%= "#{control_action_label} #{label_name('for')} " %><%= cup_link @cup %></h2>

		<div class="inner">

		<% form_for :challenge, :url => challenges_path, :html => { :class => :form } do |f| -%>
		<%= f.hidden_field :cup_id if @cup %>
		<%= render :partial => "form", :locals => {:f => f} %>
		<% end -%>

		</div>

	</div>

</div>

