<div class="block">

	<%= render :partial => '/cups/secondary_navigation' %>  

	<div class="content">  

		<h2 class="title"><%= "#{control_action_label} #{label_name('for')} " %><%= cup_link @cup %></h2>

		<div class="inner">

			<% form_for(@stage, :html => { :multipart => true, :class => :form  }) do |f| %>
				<%= hidden_field :cup, :id %>
				<%= render :partial => "form", :locals => {:f => f} %>
			<% end -%>

		</div>

	</div>

</div>
