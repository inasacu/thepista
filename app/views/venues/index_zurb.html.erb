<%
	the_label = label_name(:venues)
	content_for(:title, the_label)
%>

<div class="row">
	 
	<div class="twelve column">  
			<% if object_counter(@venues) > 0 %>
				<%= render :partial => '/venues/index_full' %>
			<% else %>
				<%= render :partial => '/venues/index_blank' %>
			<% end %>
	</div>	
</div>

<% content_for :sidebar, render(:partial => 'venues/sidebar')  if the_maximo -%>