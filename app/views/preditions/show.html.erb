<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', preditions_path  %></li>
      <li><%= link_to 'New', new_predition_path %></li>
      <li class="active"><%= link_to 'Show', predition_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Porra_id:</b>
          <%= @predition.porra_id %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @predition.user_id %>
        </p>

        <p>
          <b>Game_id:</b>
          <%= @predition.game_id %>
        </p>

        <p>
          <b>Home_score:</b>
          <%= @predition.home_score %>
        </p>

        <p>
          <b>Away_score:</b>
          <%= @predition.away_score %>
        </p>
      <%= link_to 'Edit', edit_predition_path(@predition) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>