<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', invitations_path  %></li>
      <li><%= link_to 'New', new_invitation_path %></li>
      <li class="active"><%= link_to 'Show', invitation_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Email_addresses:</b>
          <%= @invitation.email_addresses %>
        </p>

        <p>
          <b>Message:</b>
          <%= @invitation.message %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @invitation.user_id %>
        </p>

        <p>
          <b>Archive:</b>
          <%= @invitation.archive %>
        </p>

        <p>
          <b>Deleted_at:</b>
          <%= @invitation.deleted_at %>
        </p>
      <%= link_to 'Edit', edit_invitation_path(@invitation) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>