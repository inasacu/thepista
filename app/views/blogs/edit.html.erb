<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', blogs_path  %></li>
      <li><%= link_to 'New', new_blog_path %></li>
      <li class="active"><%= link_to label_name(:edit), edit_blog_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">Edit Blog</h2>
    <div class="inner">
      <% form_for @blog, :url => blog_path(@blog), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>