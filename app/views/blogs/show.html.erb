<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', blogs_path  %></li>
      <li><%= link_to 'New', new_blog_path %></li>
      <li class="active"><%= link_to 'Show', blog_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Name:</b>
          <%= @blog.name %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @blog.user_id %>
        </p>

        <p>
          <b>Group_id:</b>
          <%= @blog.group_id %>
        </p>

        <p>
          <b>Entries_count:</b>
          <%= @blog.entries_count %>
        </p>

        <p>
          <b>Description:</b>
          <%= @blog.description %>
        </p>
      <%= link_to label_name(:edit), edit_blog_path(@blog) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>