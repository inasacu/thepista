<% if DISPLAY_TRUESKILL %>
<tr>
	<% 5.times do |i| %>
	<%= content_tag :td, "&nbsp;", :class => "no_line" %>
	<% end %>
<tr>
	<%= content_tag :th, label_name(:groups), :class => "first" %>
	<%= content_tag :th, "&nbsp;", :class => "heading" %>
	<%= content_tag :th, label_name(:users), :class => "heading", :align => "left" %>
	<%= content_tag :th, label_name(:true_skill_mean_total), :class => "heading" %>
	<%= content_tag :th, label_name(:true_skill_mean_difference), :class => "heading" %>
	<%= content_tag :th, "&nbsp;&nbsp;", :class => "heading" %>
	<%= content_tag :th, "&nbsp;&nbsp;", :class => "heading" %>
	<%= content_tag :th, "&nbsp;&nbsp;", :class => "heading" if has_been_played %>
	<%= content_tag :th, "&nbsp;&nbsp;", :class => "heading" unless has_been_played  %>
	<%= content_tag :th, "&nbsp;", :class => "heading_#{show_right_border}" %>
	<%= content_tag :th, "&nbsp;", :class => "last" if show_right_border %>
</tr>
<tr>
	<%= content_tag(:td, link_to(@schedule.home_group, group_path(@schedule.group)), :class => "no_line") %>
	<%= content_tag(:td, home["level_image"], :class => "no_line", :align => "center") %>
	<%= content_tag(:td, home['players'], :class => "no_line", :align => "left") %>
	<%= content_tag(:td, home["level"], :class => "no_line") %>
	<%= content_tag(:td, home["level_difference"], :class => "no_line") %>
	<%= content_tag(:td, "&nbsp;", :class => "no_line") %>
</tr>
<tr>
	<%= content_tag(:td, link_to(@schedule.away_group, group_path(@schedule.group))) %>
	<%= content_tag(:td, away["level_image"], :align => "center") %>
	<%= content_tag(:td, away['players'], :align => "left") %>
	<%= content_tag(:td, away["level"]) %>
	<%= content_tag(:td, away["level_difference"]) %>
	<%= content_tag(:td, "&nbsp;") %>
	<%#= content_tag(:td, "&nbsp;") %>
	<%= content_tag(:td, "&nbsp;") if DISPLAY_PLAYER_POSITION %>
	<%= content_tag :td, "&nbsp;" if has_been_played %>
	<%= content_tag(:td, "&nbsp;") unless has_been_played  %>
	<%= content_tag(:td, "&nbsp;", :class => "last") if show_right_border %>
</tr>
</tr>
<tr><%# 5.times {|i| "#{content_tag(:td, '&nbsp;', :class => 'no_line')}"} %></tr>
<tr><%# 5.times {|i| "#{content_tag(:td, '&nbsp;', :class => 'no_line')}"} %></tr>
<% end %>