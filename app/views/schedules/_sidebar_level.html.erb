<%
the_label = "#{label_name(:previous_profile_set_full) }"
the_label_set = "#{label_name(:profile_set_full) }"
the_label_user_set = "#{label_name(:profile_user_set_full) }"
the_confirmation = "#{the_label}?"	
the_confirmation_user_set = "#{the_label_user_set}?"
%>

<div class="block">

	<h3><%= label_name(:options) %></h3>
	
<ul class="navigation">
	<li><%= link_to(label_name(:profile_set), set_profile_url(:id => schedule), 
				:title => the_label_set) %></li>

	<li><%= link_to(label_name(:previous_profile_set), set_previous_profile_url(:id => schedule), 
				:title => the_label, :confirm => the_confirmation) %></li>
				
	<li><%= link_to(label_name(:profile_user_set), set_user_profile_url(:id => schedule), 
			:title => the_label_user_set, :confirm => the_confirmation_user_set) %></li>
