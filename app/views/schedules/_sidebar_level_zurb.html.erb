<%
	the_label = "#{label_name(:previous_profile_set_full) }"
	the_label_set = "#{label_name(:profile_set_full) }"
	the_label_user_set = "#{label_name(:profile_user_set_full) }"
	the_confirmation = "#{the_label}?"	
	the_confirmation_user_set = "#{the_label_user_set}?"
	is_maximo = the_maximo
	
	the_label = label_name(:options)
%>


<%= set_title_class(the_label) %>
<hr/>

<div class="row">
	<div class="twelve columns">

		<% if is_maximo %>

		<p>
			<%= option_image_link('user') %>&nbsp;&nbsp;
			<%= link_to(label_name(:profile_set) , star_rate_path(:id => schedule)) %>
		</p>

		<p>
			<%= option_image_link('user_update') %>&nbsp;&nbsp;
			<%= link_to(label_name(:previous_profile_set), set_previous_profile_url(:id => schedule), :title => the_label, :confirm => the_confirmation) %>
		</p>

		<p>
			<%= option_image_link('user_update') %>&nbsp;&nbsp;
			<%= link_to(label_name(:profile_user_set), set_user_profile_url(:id => schedule), :title => the_label_user_set, :confirm => the_confirmation_user_set) %>
		</p>

		<% end -%>

		<p>
			<%= option_image_link('user_update') %>&nbsp;&nbsp;
			<%= link_to(label_name(:profile_set), set_profile_url(:id => schedule), :title => the_label_set) %>
		</p>

	</div>

</div>