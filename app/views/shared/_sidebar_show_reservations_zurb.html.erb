- installations = @venue.installations
- if object_counter(installations) > 0
  .block
    %h3= label_name(:reservations)
    .content
      - installations.each do |installation|
      - the_label = h(installation.name)
        %p
          = option_image_link('installation')
          \&nbsp;&nbsp;
          \#{link_to the_label, reservations_path(:id => installation)}
