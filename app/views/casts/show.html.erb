<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', casts_path  %></li>
      <li><%= link_to 'New', new_cast_path %></li>
      <li class="active"><%= link_to 'Show', cast_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Challenge_id:</b>
          <%= @cast.challenge_id %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @cast.user_id %>
        </p>

        <p>
          <b>Game_id:</b>
          <%= @cast.game_id %>
        </p>

        <p>
          <b>Home_score:</b>
          <%= @cast.home_score %>
        </p>

        <p>
          <b>Away_score:</b>
          <%= @cast.away_score %>
        </p>
      <%= link_to 'Edit', edit_cast_path(@cast) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>