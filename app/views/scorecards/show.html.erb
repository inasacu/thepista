<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', scorecards_path  %></li>
      <li><%= link_to 'New', new_scorecard_path %></li>
      <li class="active"><%= link_to 'Show', scorecard_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Group_id:</b>
          <%= @scorecard.group_id %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @scorecard.user_id %>
        </p>

        <p>
          <b>Wins:</b>
          <%= @scorecard.wins %>
        </p>

        <p>
          <b>Draws:</b>
          <%= @scorecard.draws %>
        </p>

        <p>
          <b>Losses:</b>
          <%= @scorecard.losses %>
        </p>

        <p>
          <b>Points:</b>
          <%= @scorecard.points %>
        </p>

        <p>
          <b>Ranking:</b>
          <%= @scorecard.ranking %>
        </p>

        <p>
          <b>Played:</b>
          <%= @scorecard.played %>
        </p>

        <p>
          <b>Assigned:</b>
          <%= @scorecard.assigned %>
        </p>

        <p>
          <b>Goals_for:</b>
          <%= @scorecard.goals_for %>
        </p>

        <p>
          <b>Goals_against:</b>
          <%= @scorecard.goals_against %>
        </p>

        <p>
          <b>Goals_scored:</b>
          <%= @scorecard.goals_scored %>
        </p>

        <p>
          <b>Previous_points:</b>
          <%= @scorecard.previous_points %>
        </p>

        <p>
          <b>Previous_ranking:</b>
          <%= @scorecard.previous_ranking %>
        </p>

        <p>
          <b>Previous_played:</b>
          <%= @scorecard.previous_played %>
        </p>

        <p>
          <b>Payed:</b>
          <%= @scorecard.payed %>
        </p>

        <p>
          <b>Archive:</b>
          <%= @scorecard.archive %>
        </p>

        <p>
          <b>Deleted_at:</b>
          <%= @scorecard.deleted_at %>
        </p>
      <%= link_to 'Edit', edit_scorecard_path(@scorecard) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>